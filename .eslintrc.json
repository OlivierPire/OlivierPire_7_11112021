{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            ":": "before"
          }
        }
      ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "linebreak-style" : ["error", "windows"],
        "quotes": [2, "double", { "avoidEscape": true }]
    }
}
